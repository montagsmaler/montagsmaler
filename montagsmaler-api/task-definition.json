{
  "family": "montagsmaler",
  "executionRoleArn": "arn:aws:iam::936738124201:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "montagsmaler-redis",
      "image": "redis",
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379,
          "protocol": "tcp"
        }
      ],
      "essential": true
    },
    {
      "name": "montagsmaler-api",
      "image": "323793774129.dkr.ecr.us-east-1.amazonaws.com/montagsmaler:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        },
        {
          "containerPort": 443,
          "hostPort": 443,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "REDIS_HOST",
          "value": "montagsmaler-redis"
        },
        {
          "name": "LISTENER_PORT",
          "value": "80"
        },
        {
          "name": "USER_POOL_ID",
          "value": "us-east-1_LKnyH8xzJ"
        },
        {
          "name": "CLIENT_ID",
          "value": "imrcgq4gedp29idmtvaq97nsv"
        },
        {
          "name": "AWS_S3_BUCKET",
          "value": "dev4cloudmontagsmalerlucas"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
