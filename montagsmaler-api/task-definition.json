{
  "family": "montagsmaler",
  "executionRoleArn": "arn:aws:iam::323793774129:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "montagasmaler-api",
      "image": "323793774129.dkr.ecr.us-east-1.amazonaws.com/montagsmaler:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
